<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent">

	<LinearLayout
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:orientation="vertical"
		tools:context=".MainActivity">

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="LAB 8-11"
			android:textSize="30sp"
			android:textStyle="bold"
			app:layout_constraintBottom_toTopOf="@id/lab_8_1"
			app:layout_constraintEnd_toEndOf="parent"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintTop_toTopOf="parent" />

		<Button
			android:id="@+id/lab_8_1"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_margin="20dp"
			android:text="8.1"
			app:layout_constraintBottom_toTopOf="@id/lab_8_2" />

		<Button
			android:id="@+id/lab_8_2"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_margin="20dp"
			android:text="8.2"
			app:layout_constraintBottom_toTopOf="@id/lab_8_3" />

		<Button
			android:id="@+id/lab_8_3"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_margin="20dp"
			android:text="8.3"
			app:layout_constraintBottom_toTopOf="@id/lab_9_1" />

		<Button
			android:id="@+id/lab_9_1"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_margin="20dp"
			android:text="9.1"
			app:layout_constraintBottom_toTopOf="@id/lab_10" />

		<Button
			android:id="@+id/lab_10"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_margin="20dp"
			android:text="10"
			app:layout_constraintBottom_toTopOf="@id/lab_11" />

		<Button
			android:id="@+id/lab_11"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_margin="20dp"
			android:text="11"
			app:layout_constraintBottom_toBottomOf="parent" />

		<Button
			android:id="@+id/lab_12"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_margin="20dp"
			android:text="12"
			app:layout_constraintBottom_toBottomOf="parent" />

		<Button
			android:id="@+id/lab_13"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_margin="20dp"
			android:text="13"
			app:layout_constraintBottom_toBottomOf="parent" />

		<Button
			android:id="@+id/lab_14"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_margin="20dp"
			android:text="14"
			app:layout_constraintBottom_toBottomOf="parent" />
	</LinearLayout>
</ScrollView>